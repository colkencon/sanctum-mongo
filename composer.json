{
    "name": "colkencon/sanctum-mongo",
    "description": "Laravel Sanctum port for compatibility for Laravel MongoDB.",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Zach.Lu",
            "email": "1049655193@qq.com"
        },
        {
            "name": "Kenneth ColkenConsult",
            "email": "info@colkenconsult.com"
        }
    ],
    "support": {
        "issues": "https://github.com/colkencon/sanctum-mongo/issues",
        "source": "https://github.com/colkencon/sanctum-mongo"
    },
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^6.9|^7.0|^8.0",
        "illuminate/database": "^6.9|^7.0|^8.0",
        "illuminate/support": "^6.9|^7.0|^8.0",
        "jenssegers/mongodb": "^3.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "ColkenCon\\Sanctum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ColkenCon\\Sanctum\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev."
        },
        "laravel": {
            "providers": [
                "ColkenCon\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
